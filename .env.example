# Generation backend selection (defaults to "stub")
# HANDWRITE_PROVIDER=stub
# HANDWRITE_PROVIDER=fastapi

# FastAPI proxy target (enables fastapi mode automatically if set)
# FASTAPI_GENERATE_URL=http://localhost:8000/generate

# Timeout for FastAPI requests in milliseconds (default: 20000)
# FASTAPI_TIMEOUT_MS=20000

# Local Python generator options
# HANDWRITE_LOCAL_USE_CLASSIFIER=1

# Auto-enable learned letter model if weights exist (default: 1)
# HANDWRITE_LOCAL_USE_LETTER_MODEL=1

# Override learned letter model weights path for local provider
# HANDWRITE_LOCAL_LETTER_MODEL_PATH=out/letter_gen.pt
